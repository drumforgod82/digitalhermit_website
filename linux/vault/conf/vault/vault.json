{
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "listener": {
    "tcp":{
      "address": "0.0.0.0:8200",
      "tls_disable": 0,
      "tls_cert_file": "/vault/config/vault.crt",
      "tls_key_file": "/vault/config/vault.key"
    }
  },
  "ui": true,
  "disable_mlock": true
}
